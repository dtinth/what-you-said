{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "what-you-said",
  "title": "What you said",
  "description": "See, copy, or type what you just said",
  "icon": "command-icon.png",
  "author": "dtinth",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "View Transcript",
      "description": "See, copy, or type what you just said",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@nanostores/react": "^0.7.1",
    "@raycast/api": "^1.55.2",
    "@raycast/utils": "^1.4.8",
    "chalk": "^5.3.0",
    "debug": "^4.3.4",
    "fastify": "^4.20.0",
    "nanostores": "^0.9.3"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "node-fetch": "^3.3.1",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
